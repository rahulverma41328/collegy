<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/dashboard_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.collegeconnected.fragments.dashboard.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_communityFragment"
            app:destination="@id/communityFragment" />
    </fragment>
    <fragment
        android:id="@+id/communityFragment"
        android:name="com.example.collegeconnected.fragments.dashboard.CommunityFragment"
        android:label="CommunityFragment" />
    <fragment
        android:id="@+id/addPostFragment"
        android:name="com.example.collegeconnected.fragments.dashboard.AddPostFragment"
        android:label="AddPostFragment" />
    <fragment
        android:id="@+id/notificationsFragment"
        android:name="com.example.collegeconnected.fragments.dashboard.NotificationsFragment"
        android:label="NotificationsFragment" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.collegeconnected.fragments.dashboard.ProfileFragment"
        android:label="ProfileFragment" />
</navigation>